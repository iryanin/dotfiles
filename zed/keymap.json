[
  {
    "context": "Editor && vim_mode == normal && vim_operator == none && !VimWaiting",
    "bindings": {
      "space w": "workspace::Save",
      "s v": "pane::SplitVertical",
      "s h": "pane::SplitHorizontal",
      "s c": "pane::CloseActiveItem",
      "s o": "pane::CloseOtherItems",
      "ctrl-j": "workspace::ActivatePaneDown",
      "ctrl-k": "workspace::ActivatePaneUp",
      "ctrl-h": "workspace::ActivatePaneLeft",
      "ctrl-l": "workspace::ActivatePaneRight",
      "g d": "editor::GoToDefinition",
      "g r": "editor::FindAllReferences",
      "g I": "editor::GoToImplementation",
      "g y": "editor::GoToTypeDefinition",
      "g D": "editor::GoToDeclaration",
      "K": "editor::Hover",
      "g K": "editor::ShowSignatureHelp",
      "[ d": "project_panel::SelectPrevDiagnostic",
      "] d": "project_panel::SelectNextDiagnostic",
      "space f": "editor::Format",
      "space o": "outline::Toggle",
      "space e": "pane::RevealInProjectPanel",
      "space c a": "editor::ToggleCodeActions",
      "space r n": "editor::Rename",
      "space f f": "file_finder::Toggle",
      "space f g": "pane::DeploySearch",
      "[ c": "editor::GoToPreviousHunk",
      "] c": "editor::GoToHunk",
      "space h d": "editor::ExpandAllDiffHunks",
      "ctrl-`": "terminal_panel::Toggle"
    }
  },
  {
    "context": "ProjectPanel && not_editing",
    "bindings": {
      "space e": "workspace::ToggleLeftDock",
      "a": "project_panel::NewFile",
      "r": "project_panel::Rename",
      "d": "project_panel::Delete",
      "D": "project_panel::Trash",
      "c": "project_panel::Copy",
      "p": "project_panel::Paste"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-`": "terminal_panel::Toggle"
    }
  }
]
